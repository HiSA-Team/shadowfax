set confirm off
set breakpoint pending on
set pagination off
set arch riscv:rv64

target remote localhost:1234

file target/riscv64imac-unknown-none-elf/debug/shadowfax

# load tsm elf file for debug symbols
add-symbol-file target/riscv64imac-unknown-none-elf/debug/shadowfax_tsm 0x80A00000

# break shadowfax_core::trap::handler
break shadowfax_tsm::main
